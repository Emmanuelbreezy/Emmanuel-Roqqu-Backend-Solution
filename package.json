{
  "name": "roqq",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --files src/index.ts",
    "build": "tsc && cp ./package.json ./dist && cp ./src/database/db.sqlite ./dist/database",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/database.config.ts",
    "db:generate": "npm run typeorm -- migration:generate src/database/migrations/NewMigration",
    "db:migrate": "npm run typeorm -- migration:run",
    "db:revert": "npm run typeorm -- migration:revert",
    "db:drop": "npm run typeorm -- schema:drop",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unit": "jest --testPathPattern=services/__tests__",
    "test:api": "jest --testPathPattern=controllers/__tests__",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "passport": "^0.7.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/passport": "^1.0.17",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.7.3"
  }
}
